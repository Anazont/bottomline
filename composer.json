{
    "name": "maciejczyzewski/bottomline",
    "description": "A full-on PHP manipulation utility-belt that provides support for the usual functional.",
    "keywords": ["__", "bottomline", "library", "utility", "functions"],
    "type": "library",
    "require": {
        "php": ">=5.5.0",
        "ext-mbstring": "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Maciej A. Czyzewski",
            "email": "maciejanthonyczyzewski@gmail.com"
        }
    ],
    "autoload": {
        "files": ["bottomline.php"],
        "psr-4": {"__\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "__\\Test\\": "tests/__/",
            "__\\Test\\Utilities\\": "tests/Utilities/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "doctrine/instantiator": "~1.0.5",
        "phpdocumentor/reflection-docblock": "^2.0||^3.1",
        "erusev/parsedown": "^1.7",
        "nikic/php-parser": "^2.0||^3.0",
        "friendsofphp/php-cs-fixer": "^2.14"
    },
    "scripts": {
        "test": [
            "# Silence PHP deprecation warnings before PHPUnit is run",
            "php -d error_reporting=24575 ./vendor/bin/phpunit"
        ]
    }
}
